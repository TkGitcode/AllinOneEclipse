<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>
			FileUpload
		</servlet-name>
		<servlet-class>com.applicationform.FileServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ApplicationFormServlet</servlet-name>
		<display-name>ApplicationFormServlet</display-name>
		<description></description>
		<servlet-class>com.servletapplicationform.ApplicationFormServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>FileUpload</servlet-name>
	<url-pattern>/appform</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ApplicationFormServlet</servlet-name>
		<url-pattern>/applicationformservlet</url-pattern>
	</servlet-mapping>
</web-app>


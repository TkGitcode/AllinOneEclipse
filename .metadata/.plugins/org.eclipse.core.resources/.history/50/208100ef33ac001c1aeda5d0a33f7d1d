package com.practice;

public class InsertionSort {

	public static void main(String[] args) {
		int a[] = { 1, 3, 2, 5, 77, 4, 2 };

		for (int i = 0; i < a.length; i++) {
			for (int j = 0; j < a.length; j++) {
				if(a[i]<a[j])
				{
					int temp=a[j];
					a[j]=a[i];
					a[i]=temp;
				}

			}
			
			}
		for(int i=0;i<a.length;i++)
		{
			System.out.println(a[i]+" ");
		}

	}

}
